{
  "name": "github-webhook-dispatcher",
  "version": "3.0.0",
  "description": "API endpoint to route GitHub webhooks to specified targets",
  "main": "app.js",
  "scripts": {
    "start": "npx node dist/index.js",
    "test": "npm run test:lint && npm run test:spell && npm run test:markdown && npm run test:unit",
    "test:lint": "eslint",
    "test:spell": "cspell *.md",
    "test:markdown": "npx markdownlint-cli -c markdownlint.yml *.md",
    "test:unit": "jest",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage",
    "build": "ncc build src/app.js --license licenses.txt",
    "dev": "npx nodemon src/app.js"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": [
      "**/test/**/*.test.js"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ]
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=20.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collinmcneese/github-webhook-dispatcher.git"
  },
  "keywords": [
    "github",
    "webhook"
  ],
  "author": "Collin McNeese <collinmcneese@github.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/collinmcneese/github-webhook-dispatcher/issues"
  },
  "homepage": "https://github.com/collinmcneese/github-webhook-dispatcher#readme",
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@vercel/ncc": "^0.38.3",
    "cspell": "^9.2.0",
    "eslint": "^9.33.0",
    "eslint-config-strongloop": "^2.1.0",
    "jest": "^30.0.5",
    "js-yaml": "^4.1.0",
    "markdownlint-cli": "^0.45.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4"
  },
  "dependencies": {
    "dotenv": "^17.2.1",
    "express": "^5.1.0",
    "express-rate-limit": "^8.0.1",
    "helmet": "^8.1.0",
    "toml": "^3.0.0"
  }
}
